{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Game Config",
  "version": "0.1.0",
  "identifier": "com.root.game-config",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Game Config",
        "width": 1202,
        "height": 780
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/logo.ico"
    ]
  }
}
